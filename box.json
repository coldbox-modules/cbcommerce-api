{
    "name":"cbox-commerce",
    "version":"0.0.1",
    "author":"",
    "location":"jclausen/cbox-commerce#v0.0.1",
    "homepage":"https://github.com/jclausen/cbox-commerce",
    "documentation":"https://github.com/jclausen/cbox-commerce",
    "repository":{
        "type":"git",
        "URL":"https://github.com/jclausen/cbox-commerce"
    },
    "bugs":"https://github.com/jclausen/cbox-commerce/issues",
    "slug":"cbox-commerce",
    "shortDescription":"An e-commerce module for the coldbox platform",
    "description":"An e-commerce module for the coldbox platform",
    "type":"modules",
    "dependencies":{
        "cfmigrations":"^1.3.1",
        "quick":"^1.0.0",
        "cffractal":"6.1.2",
        "cbsecurity":"^1.3.0",
        "cbstorages":"^1.4.0",
        "cbguard":"^1.0.0",
        "BCrypt":"^2.5.0-snapshot"
    },
    "devDependencies":{
        "coldbox":"^5.0.0",
        "testbox":"^2.4.0+80"
    },
    "installPaths":{
        "testbox":"testbox/",
        "coldbox":"tests/resources/app/coldbox/",
        "quick":"modules/quick/",
        "cfmigrations":"modules/cfmigrations/",
        "cffractal":"modules/cffractal/",
        "cbsecurity":"modules/cbsecurity/",
        "cbstorages":"modules/cbstorages/",
        "cbguard":"modules/cbguard/",
        "BCrypt":"modules/BCrypt/"
    },
    "scripts":{
        "postVersion":"package set location='jclausen/cbox-commerce#v`package version`'",
        "onRelease":"publish",
        "postPublish":"!git push && !git push --tags"
    },
    "ignore":[
        "**/.*",
        "test",
        "tests"
    ],
    "cfmigrations":{
        "connectionInfo":{
            "password":"${DB_PASSWORD}",
            "connectionString":"${DB_CONNECTIONSTRING}",
            "class":"${DB_CLASS}",
            "username":"${DB_USER}"
        },
        "schema":"${DB_DATABASE}",
        "defaultGrammar":"MySQLGrammar"
    }
}